{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.1",
        "beberlei/DoctrineExtensions": "^1.2",
        "cocur/slugify": "^3.2",
        "doctrine/inflector": "^1.3",
        "easycorp/easyadmin-bundle": "^2.1",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "friendsofsymfony/rest-bundle": "^2.5",
        "friendsofsymfony/user-bundle": "~2.0",
        "google/cloud-translate": "^1.2",
        "jms/serializer-bundle": "^2.4",
        "knplabs/knp-menu-bundle": "^2.2",
        "knplabs/knp-snappy-bundle": "^1.5",
        "lexik/jwt-authentication-bundle": "^2.6",
        "liip/imagine-bundle": "^2.0",
        "moskalyovd/fcm-bundle": "^0.3.0",
        "overblog/graphql-bundle": "^0.11.10",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/filesystem": "4.2.*",
        "symfony/finder": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.2.*",
        "symfony/http-client": "4.4.2",
        "symfony/lock": "4.2.*",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "4.2.*",
        "symfony/serializer": "4.2.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/web-server-bundle": "4.2.*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "4.2.*",
        "tattali/calendar-bundle": "1.1.8",
        "vich/uploader-bundle": "^1.8",
        "willdurand/faker-bundle": "^1.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "4.2.*"
        }
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.1",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "fzaninotto/faker": "^1.8",
        "overblog/graphiql-bundle": "^0.1.2",
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.13",
        "symfony/var-dumper": "4.2.*"
    }
}
